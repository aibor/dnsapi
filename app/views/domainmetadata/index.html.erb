<h1>Listing domainmetadata</h1>

<table class="table">
  <thead>
    <tr>
      <th>Domain</th>
      <th>Kind</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domainmetadata.each do |domainmetadatum| %>
      <tr>
        <td><%= domainmetadatum.domain %></td>
        <td><%= domainmetadatum.kind %></td>
        <td><%= domainmetadatum.content %></td>
        <td><%= link_to 'Show', domainmetadatum %></td>
        <td><%= link_to 'Edit', edit_domainmetadatum_path(domainmetadatum) %></td>
        <td><%= link_to 'Destroy', domainmetadatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Domainmetadatum', new_domainmetadatum_path %>
